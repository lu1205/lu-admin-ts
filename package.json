    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "dayjs": "^1.11.9",
    "element-plus": "^2.3.9",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
